<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<shortcuts>
    <skipIfNotSupported/>
    <programGroup defaultName="Czsem" location="applications"/>
    <shortcut KdeUsername="root" applications="no"
        commandLine="-jar &quot;$INSTALL_PATH/$czsem_main_jar_loc&quot;"
        desktop="no" encoding="UTF-8" initialState="normal"
        name="Run Weka with Fuzzy ILP" programGroup="yes" startMenu="no"
        startup="no" target="java" type="Application" workingDirectory="$INSTALL_PATH">
            	
            	<createForPack name="Weka integration"/>
    </shortcut>
        
    <shortcut KdeUsername="root" applications="no"
        desktop="no" initialState="normal" name="Czsem Website online"
        programGroup="yes" startMenu="no" startup="no" target="http://czsem.berlios.de/" url="http://czsem.berlios.de/" type="Link"/>
    <shortcut KdeUsername="root" applications="no"
        commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/Uninstaller.jar&quot;"
        desktop="no" encoding="UTF-8" initialState="normal"
        name="Uninstall Czsem" programGroup="yes" startMenu="no"
        startup="no" target="java" type="Application"/>
</shortcuts>
